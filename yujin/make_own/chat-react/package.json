    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "http": "0.0.1-security",
    "minimist": "1.2.5",
    "mysql2": "^2.2.5",
    "nodemon": "2.0.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-modal": "^3.14.3",
    "react-redux": "7.2.4",
    "react-scripts": "3.2.0",
    "redis": "3.1.2",
    "redux": "4.1.1",
    "redux-saga": "^1.1.3",
    "socket.io": "4.1.3",
